        <h1 class="text-primary md:text-[35px] lg:text-[50px] text-[30px] font-bold text-center xl:text-left lang" key="14-day-free-trial">Register your Clinic<br />for a 14-Days FREE TRIAL</h1>
        <div class="md:flex flex-row justify-center items-center gap-6 lg:mr-[-50px] space-y-3 lg:space-y-0">
            <a href="#" class="btn bg-primary text-white min-w-[330px] text-center lang" key="register-now">Register Now</i></a>
            <a href="#" class="btn bg-white text-primary min-w-[330px] border border-primary text-center">Whatsapp Sales</a>
        </div>
    </div>
</section>

@Html.Partial("_FooterPartial")
@section Scripts{
    <script type="text/javascript">
        $.ajax({
            url: '/GetPrices',
            success: (data) => {
                console.log(data);

                const threeMonth = data.filter((item) => item.totalMonth == 3);
                const sixMonth = data.filter((item) => item.totalMonth == 6);
                const oneYear = data.filter((item) => item.totalMonth == 12);

                const threeMonthElement = $('.tab-content .tab-item').eq(0);
                $(threeMonthElement).find('.practice').find('.price').text(`Rp ${formatNumber(threeMonth[0].price)}/`);
                $(threeMonthElement).find('.practice').find('.discounted-price').text(`Rp ${formatNumber(threeMonth[0].discountedPrice)}`);
                $(threeMonthElement).find('.practice').find('.discount-amount').text(`${threeMonth[0].initialDiscount}`);
                $(threeMonthElement).find('.clinic').find('.price').text(`Rp ${formatNumber(threeMonth[1].price)}/`);
                $(threeMonthElement).find('.clinic').find('.discounted-price').text(`Rp ${formatNumber(threeMonth[1].discountedPrice)}`);
                $(threeMonthElement).find('.clinic').find('.discount-amount').text(`${threeMonth[1].initialDiscount}`);

                const sixMonthElement = $('.tab-content .tab-item').eq(1);
                $(sixMonthElement).find('.practice').find('.price').text(`Rp ${formatNumber(sixMonth[0].price)}/`);
                $(sixMonthElement).find('.practice').find('.discounted-price').text(`Rp ${formatNumber(sixMonth[0].discountedPrice)}`);
                $(sixMonthElement).find('.practice').find('.discount-amount').text(`${sixMonth[0].initialDiscount}`);
                $(sixMonthElement).find('.clinic').find('.price').text(`Rp ${formatNumber(sixMonth[1].price)}/`);
                $(sixMonthElement).find('.clinic').find('.discounted-price').text(`Rp ${formatNumber(sixMonth[1].discountedPrice)}`);
                $(sixMonthElement).find('.clinic').find('.discount-amount').text(`${sixMonth[1].initialDiscount}`);

                const oneYearElement = $('.tab-content .tab-item').eq(2);
                $(oneYearElement).find('.practice').find('.price').text(`Rp ${formatNumber(oneYear[0].price)}/`);
                $(oneYearElement).find('.practice').find('.discounted-price').text(`Rp ${formatNumber(oneYear[0].discountedPrice)}`);
                $(oneYearElement).find('.practice').find('.discount-amount').text(`${oneYear[0].initialDiscount}`);
                $(oneYearElement).find('.clinic').find('.price').text(`Rp ${formatNumber(oneYear[1].price)}/`);
                $(oneYearElement).find('.clinic').find('.discounted-price').text(`Rp ${formatNumber(oneYear[1].discountedPrice)}`);
                $(oneYearElement).find('.clinic').find('.discount-amount').text(`${oneYear[1].initialDiscount}`);
            },
        });
    </script>
}